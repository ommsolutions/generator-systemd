    "fs-extra": "^4.0.1",
    "yeoman-generator": "^1.0.0",
    "yosay": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns" : ["/node_modules/", "/coverage/"]
